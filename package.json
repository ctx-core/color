{
	"name": "@ctx-core/color",
	"version": "11.0.2",
	"description": "ctx-core color",
	"keywords": [
		"ctx-core",
		"color"
	],
	"homepage": "https://github.com/ctx-core/color#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/color/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/color/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
